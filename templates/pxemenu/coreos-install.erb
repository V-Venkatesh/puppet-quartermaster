LABEL <%= @name %>
        MENU LABEL (<%= @release %>-<%= @p_arch %>): CoreOS ^Manual Installation 
        IPAPPEND 2
        KERNEL ../<%= @distro %>/<%= @p_arch %>/<%= @target_kernel %>
        INITRD ../<%= @distro %>/<%= @p_arch %>/<%= @target_initrd %>
        APPEND coreos.autologin=tty1

LABEL <%= @name %>
        MENU LABEL (<%= @release %>-<%= @p_arch %>): CoreOS ^Unattended Installation 
        IPAPPEND 2
        KERNEL ../<%= @distro %>/<%= @p_arch %>/<%= @target_kernel %>
        INITRD ../<%= @distro %>/<%= @p_arch %>/<%= @target_initrd %>
        APPEND coreos.config.url=http://<%= @fqdn %>/<%= @distro %>/<%= @autofile %>/<%= @name %>.<%= @autofile %>
